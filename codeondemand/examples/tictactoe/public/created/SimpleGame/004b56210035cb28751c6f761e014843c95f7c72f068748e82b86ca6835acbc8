"use strict";

function SimpleGame(props) {
    console.log("in simple game", props);
    return React.createElement(
        "div",
        { className: "game" },
        React.createElement(Board, { context: props.context.focusOn("_embedded").focusOn('board') })
    );
}

function Board(props) {
    var squares = props.context.focusOn('squares');
    var sq = function sq(n) {
        return React.createElement(Square, { context: squares.withLens(Lens.nth(n)) });
    };
    return React.createElement(
        "div",
        { className: "board" },
        React.createElement(
            "div",
            null,
            sq(0),
            sq(1),
            sq(2)
        ),
        React.createElement(
            "div",
            null,
            sq(3),
            sq(4),
            sq(5)
        ),
        React.createElement(
            "div",
            null,
            sq(6),
            sq(7),
            sq(8)
        )
    );
}
function Square(props) {
    var onClick = props.context.domain.setSquareAndToggleState(props.context);
    return React.createElement(
        "button",
        { className: "square", onClick: function () {
                return onClick;
            } },
        props.context.json()
    );
}
//# sourceMappingURL=SimpleGame.js.map
return SimpleGame
