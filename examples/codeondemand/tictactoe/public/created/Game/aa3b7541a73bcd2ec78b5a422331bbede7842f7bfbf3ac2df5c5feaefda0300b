'use strict';

function Game(_ref) {
    var context = _ref.context;

    return React.createElement(
        'div',
        { className: 'game' },
        React.createElement(
            'div',
            { className: 'game-info' },
            React.createElement(
                'p',
                null,
                'Next turn is ',
                context.json().state
            ),
            React.createElement(
                'ul',
                null,
                React.createElement(LoadGame, { context: context.focusOn('_links').focusOn('game1') }),
                React.createElement(LoadGame, { context: context.focusOn('_links').focusOn('game2') })
            )
        ),
        React.createElement(
            'div',
            { className: 'game-board' },
            React.createElement(ComponentFromServer, { context: context.focusOn('_embedded').focusOn('board') })
        )
    );
}

function LoadGame(props) {
    var url = props.context.json().href;
    var onclick = function onclick() {
        return props.context.domain.loadJson(url);
    };
    return React.createElement(
        'li',
        null,
        React.createElement(
            'a',
            { onClick: onclick },
            url
        )
    );
}
//# sourceMappingURL=Game.js.map
return Game
