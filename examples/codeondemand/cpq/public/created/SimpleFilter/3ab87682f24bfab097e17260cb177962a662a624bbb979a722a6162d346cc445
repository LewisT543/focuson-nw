"use strict";

function SimpleFilter(props) {
    var onChange = function onChange(event) {
        console.log("onChange.target", event.target.value);
    };
    var filterJson = props.context.json();
    var options = props.context.domain.makeOptions(filterJson.selected, props.context.json().legalValues);
    return React.createElement(
        "div",
        { key: filterJson.filterName, className: filterJson.filterName },
        React.createElement(
            "p",
            null,
            React.createElement(
                "span",
                { className: "font-weight-bold" },
                "Filter:"
            ),
            " ",
            filterJson.filterName
        ),
        React.createElement(
            "select",
            { className: "custom-select custom-select-md mb-3", onChange: function (event) {
                    return onChange(event);
                }, key: "select-" + filterJson.filterName },
            options
        )
    );
}
//# sourceMappingURL=SimpleFilter.js.map
return SimpleFilter
